{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/index.js",
        "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS", "PATCH"]
      }
    ],
    "rewrites": [
    { "source": "/api/(.*)", "destination": "/src/index.js" }
  ]
}
  
  