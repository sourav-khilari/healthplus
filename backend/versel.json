{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@versel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/index.js",
        "methods": ["GET", "POST", "PUT", "DELETE","OPTIONS","PATCH"]
      }
    ]
}